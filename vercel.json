{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/friends",
      "dest": "/friends.html"
    },
    {
      "src": "/tasks",
      "dest": "/tasks.html"
    },
    {
      "src": "/market",
      "dest": "/market.html"
    },
    {
      "src": "/softie",
      "dest": "/softie.html"
    },
    {
      "src": "/more",
      "dest": "/more.html"
    },
    {
      "src": "/upgrades",
      "dest": "/upgrades.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/register",
      "dest": "/register.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
